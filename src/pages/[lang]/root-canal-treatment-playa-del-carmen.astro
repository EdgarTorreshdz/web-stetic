---
export function getStaticPaths() {
  return [
    { params: { lang: 'en' } },
    { params: { lang: 'es' } },
  ];
}

const { lang } = Astro.params as { lang: 'en' | 'es' };
import Layout from "../../layouts/Layout.astro"
import HeaderEspecialidad from "../../components/endodoncia/HeaderEspecialidad.astro"
import AboutEspecialidad from "../../components/endodoncia/AboutEspecialidad.astro"
---

<Layout title={lang === 'es' ? "Endodoncia" : "Endodontics"} lang={lang}>
    <HeaderEspecialidad />
    <AboutEspecialidad />
</Layout>
