---
export async function getStaticPaths() {
  return [
    { params: { lang: 'en', cosmetic_dentistry: 'cosmetic-dentistry' } },
    { params: { lang: 'es', cosmetic_dentistry: 'odontologia-estetica' } },
  ];
}

const { lang } = Astro.params as { lang: 'en' | 'es' };
import Layout from "../../layouts/Layout.astro"
import HeaderEspecialidad from "../../components/odontologia-estetica/HeaderEspecialidad.astro"
import NavegacionEspecialidad from "../../components/odontologia-estetica/NavegacionEspecialidad.astro"
import BreadCrumbs from "../../components/odontologia-estetica/BreadCrumbs.astro"
import AboutEspecialidad from "../../components/odontologia-estetica/AboutEspecialidad.astro"
import AboutOtherEspecialidad from "../../components/odontologia-estetica/AboutOtherEspecialidad.astro"
import TestimoniosEspecialidad from "../../components/odontologia-estetica/TestimoniosEspecialidad.astro"
---

<Layout title={lang === 'es' ? "Odontología Estética" : "Aesthetic Dentistry"} lang={lang}>
    <HeaderEspecialidad />
    <BreadCrumbs />
    <NavegacionEspecialidad />
    <AboutEspecialidad />
    <AboutOtherEspecialidad />
    <TestimoniosEspecialidad />
</Layout>
