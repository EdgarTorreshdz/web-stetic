---
export function getStaticPaths() {
  return [
    { params: { lang: 'en' } },
    { params: { lang: 'es' } },
  ];
}

const { lang } = Astro.params as { lang: 'en' | 'es' };
import Layout from "../../layouts/Layout.astro"
import HeaderEspecialidad from "../../components/ortodoncia/HeaderEspecialidad.astro"
import NavegacionEspecialidad from "../../components/ortodoncia/NavegacionEspecialidad.astro"
import BreadCrumbs from "../../components/ortodoncia/BreadCrumbs.astro"
import AboutEspecialidad from "../../components/ortodoncia/AboutEspecialidad.astro"
import AboutOtherEspecialidad from "../../components/ortodoncia/AboutOtherEspecialidad.astro"
import TestimoniosEspecialidad from "../../components/ortodoncia/TestimoniosEspecialidad.astro"
---

<Layout>
    <HeaderEspecialidad />
    <BreadCrumbs />
    <NavegacionEspecialidad />
    <AboutEspecialidad />
    <AboutOtherEspecialidad />
    <TestimoniosEspecialidad />
</Layout>
